{
  "name": "Uniflow Client",
  "version": "1.0.2",
  "description": "Workflow your micro",
  "icons": {
    "16": "img/icon-16x16.png",
    "32": "img/icon-32x32.png",
    "48": "img/icon-48x48.png",
    "72": "img/icon-72x72.png",
    "96": "img/icon-96x96.png",
    "144": "img/icon-144x144.png",
    "192": "img/icon-192x192.png",
    "256": "img/icon-256x256.png",
    "384": "img/icon-384x384.png",
    "512": "img/icon-512x512.png"
  },
  "manifest_version": 2,
  "browser_action": {
    "default_icon": {
      "16": "img/icon-16x16.png",
      "32": "img/icon-32x32.png",
      "48": "img/icon-48x48.png",
      "72": "img/icon-72x72.png",
      "96": "img/icon-96x96.png",
      "144": "img/icon-144x144.png",
      "192": "img/icon-192x192.png",
      "256": "img/icon-256x256.png",
      "384": "img/icon-384x384.png",
      "512": "img/icon-512x512.png"
    },
    "default_popup": "popup.html",
    "default_title": "Uniflow"
  },
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*",
        "file:///*"
      ],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "<all_urls>",
    "activeTab",
    "alarms",
    "background",
    "bookmarks",
    "browsingData",
    "contentSettings",
    "contextMenus",
    "cookies",
    "debugger",
    "downloads",
    "downloads.open",
    "geolocation",
    "history",
    "identity",
    "idle",
    "management",
    "nativeMessaging",
    "notifications",
    "pageCapture",
    "privacy",
    "proxy",
    "sessions",
    "storage",
    "tabs",
    "topSites",
    "webNavigation",
    "webRequest",
    "webRequestBlocking"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
