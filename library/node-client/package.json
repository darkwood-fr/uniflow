{
  "name": "@uniflow-io/node-client",
  "version": "1.1.12",
  "description": "Uniflow node client",
  "author": "Mathieu Ledru <matyo91@gmail.com>",
  "bugs": {
    "url": "https://github.com/uniflow-io/uniflow/issues"
  },
  "homepage": "https://github.com/uniflow-io/uniflow/tree/master/library/node-client#readme",
  "keywords": [
    "uniflow",
    "uniflow-client"
  ],
  "license": "Commons Clause + MIT",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/uniflow-io/uniflow.git",
    "directory": "library/node-client"
  },
  "scripts": {
    "dev": "./node_modules/.bin/webpack-dev-server --env.NODE_ENV=development --hot --inline -d --progress --colors --config webpack.config.js",
    "build-preprod": "./node_modules/.bin/webpack --env.NODE_ENV=preprod --mode production -p --progress --colors --config webpack.config.js",
    "build": "./node_modules/.bin/webpack --env.NODE_ENV=production -p --progress --colors --config webpack.config.js",
    "format": "./node_modules/.bin/prettier --write \"src/**/*.js\"",
    "upgrade-all": "./node_modules/.bin/npx yarn-upgrade-all"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "fs-extra": "^8.1.0",
    "merge": "^1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-transform-react-jsx": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-flow": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "babel-loader": "^8.0.6",
    "npx": "^10.2.0",
    "prettier": "^1.19.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  }
}
