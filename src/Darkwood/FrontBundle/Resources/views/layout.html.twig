        <div id="navbar" class="navbar-collapse collapse">

            <div class="navbar-right infos">
                {% if app.user %}
                    <p class="text-right"><a href="{{ path('fos_user_security_logout') }}"><span class="glyphicon glyphicon-off logout" aria-hidden="true"></span></a> <a href="{{ path('fos_user_profile_edit') }}">{{ app.user.firstName }} {{ app.user.lastName }}</a></p>
                {% else %}
                    <p class="text-right"><a href="{{ path('fos_user_security_login') }}"><span class="glyphicon glyphicon-log-in login" aria-hidden="true"></span> {{ 'user.login.title' | trans }}</a></p>
                {% endif %}
            </div>
        </div>
    </div>
